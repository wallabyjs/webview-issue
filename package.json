{
	"name": "webview-issue",
	"description": "Webview Issue",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:webviewIssue"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "issue.reproduce",
				"title": "Reproduce Webview Issue",
				"category": "Webview Issue"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"eslint": "^9.13.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.11.0"
	}
}
